{
	"version": "1.0.0",
	"name": "boards-app",
	"description": "A collaborative whiteboard application with real-time features.",
	"repository": "https://github.com/Excali-Boards/boards-app",
	"author": "Digital39999",
	"type": "module",
	"scripts": {
		"build": "remix vite:build",
		"dev": "vite --host",
		"lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
		"start": "dotenv cross-env NODE_ENV=production node ./build/server/index.js",
		"rebuild": "pnpm run build && pnpm run start",
		"typecheck": "tsc --watch",
		"typecheck:once": "tsc --noEmit",
		"updates": "pnpm npm-check-updates -i --format group"
	},
	"license": "GPL-3.0",
	"pnpm": {
		"onlyBuiltDependencies": [
			"@prisma/client",
			"@prisma/engines",
			"@excali-boards/boards-api-client",
			"esbuild",
			"prisma"
		]
	},
